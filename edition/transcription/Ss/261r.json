\n<lb/><am>⁊</am><ex>e</ex> caualleros <am>⁊</am><ex>e</ex> delos mas hon<choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ado<hi rend=\"sup\">s</hi>\n<lb/>omes delos conçejos que fueron y\n<lb/>llamados <am>⁊</am><ex>e</ex> ovieron y su acuerdo\n<lb/>en vno que pues que <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey avie do\n<lb break=\"no\"/>ze an<am>̄</am><ex>n</ex>os de hedat asaz era <choice><abbr>tp<am>̄</am>o</abbr><expan>t<ex>ien</ex>po</expan></choice>\n<lb/>quele casasen <am>⁊</am><ex>e</ex> sopieron com<am>̄</am><ex>m</ex>o el\n<lb/><choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don enrric de yngla <choice><abbr>tr<am>̄</am>ra</abbr><expan>t<ex>ie</ex>rra</expan></choice> avie\n<lb/>vna fija muy fermosa de nueue\n<lb/>an<am>̄</am><ex>n</ex>os que era por casar que avie\n<lb/>nonbre don<am>̄</am><ex>n</ex>a leonor <am>⁊</am><ex>e</ex> enbiaron\n<lb/>quatro om<am>̄</am><ex>ne</ex>s buenos delos mejore<hi rend=\"sup\">s</hi>\n<lb/>dela corte apedirle <am>⁊</am><ex>e</ex> eran dos <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>i\n<lb break=\"no\"/>cos omes <am>⁊</am><ex>e</ex> los dos <choice><abbr>obp<am>̄</am>os</abbr><expan>ob<ex>is</ex>pos</expan></choice>. estos\n<lb/>se fueron <choice><abbr><am>ꝑ</am>a</abbr><expan>p<ex>ar</ex>a</expan></choice> yngla <choice><abbr>tr<am>̄</am>ra</abbr><expan>t<ex>ie</ex>rra</expan></choice> <am>⁊</am><ex>e</ex> pidiero<am>̄</am><ex>n</ex>\n<lb/>la fija al <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey <am>⁊</am><ex>e</ex> el <choice><abbr>diogl<am>̄</am>a</abbr><expan>diog<ex>e</ex>la</expan></choice> de buena\n<lb/>mente <am>⁊</am><ex>e</ex> fueron del bien <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>esçebi\n<lb break=\"no\"/>dos <am>⁊</am><ex>e</ex> enbio la mucho onrrada me<am>̄</am><ex>n</ex>\n<lb break=\"no\"/>te <choice><abbr><am>ꝑ</am>a</abbr><expan>p<ex>ar</ex>a</expan></choice> burgos <am>⁊</am><ex>e</ex> fueron fechas las\n<lb/>bodas muy <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>icas <am>⁊</am><ex>e</ex> muy nobles <am>⁊</am><ex>e</ex>\n<lb/>ovo y muchas gentes de castiella \n\n<cb n=\"b\"/>\n\n\n<lb/><am>⁊</am><ex>e</ex> de leon <am>⁊</am><ex>e</ex> de aragon <am>⁊</am><ex>e</ex> de nauarra <am>⁊</am><ex>e</ex>\n<lb/>de otras <choice><abbr><am>ꝑ</am>tes</abbr><expan>p<ex>ar</ex>tes</expan></choice> muchas alli ovo mucha<hi rend=\"sup\">s</hi>\n<lb/>grandes misiones fechas <am>⁊</am><ex>e</ex> muy gran\n<lb break=\"no\"/>des dados dados. la duen<am>̄</am><ex>n</ex>a salio\n<lb/>despues muy cuerda <am>⁊</am><ex>e</ex> muy sesuda\n<lb/><am>⁊</am><ex>e</ex> mucho entendida fiera cosa <am>⁊</am><ex>e</ex> muy\n<lb/>buena <am>⁊</am><ex>e</ex> muy loçana esta fue ermana\n<lb/>del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey enrric el jouen <am>⁊</am><ex>e</ex> del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>icart<am>̄</am><ex>e</ex>\n<lb/>que fue tanbien <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey <am>⁊</am><ex>e</ex> tan corajoso <am>⁊</am><ex>e</ex>\n<lb/>tan esforçado que mas non podrie <am>⁊</am><ex>e</ex>\n<lb/>del conde de bretania <am>⁊</am><ex>e</ex> del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey jua<am>̄</am><ex>n</ex> sy<am>̄</am><ex>n</ex>\n<lb/><choice><abbr>tr<am>̄</am>ra</abbr><expan>t<ex>ie</ex>rra</expan></choice>. esta <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>eyna don<am>̄</am><ex>n</ex>a leonor ovo\n<lb/>otrosi dos hermanas la vna fue du\n<lb break=\"no\"/>quesa de sant son<am>̄</am><ex>n</ex>a <am>⁊</am><ex>e</ex> la otra <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>eyna\n<lb/>de çeçilia. en este an<am>̄</am><ex>n</ex>o lidiaro<am>̄</am><ex>n</ex> don\n<lb/><choice><abbr><am>ꝑ</am>o</abbr><expan>p<ex>er</ex>o</expan></choice> dara<am>̄</am><ex>n</ex>çuri <am>⁊</am><ex>e</ex> don <choice><abbr><am>ꝑ</am>o</abbr><expan>p<ex>er</ex>o</expan></choice> <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>uys enlas cu\n<lb break=\"no\"/>euas de <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>onçi <am>⁊</am><ex>e</ex> fue de don <choice><abbr><am>ꝑ</am>o</abbr><expan>p<ex>er</ex>o</expan></choice> <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>uys <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>a\n<lb break=\"no\"/>ça desde el noueno an<am>̄</am><ex>n</ex>o fasta el cator\n<lb break=\"no\"/>zeno del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>egnado del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don alfon<am>̄</am><ex>so</ex>\n<lb/>no<am>̄</am><ex>n</ex> fallamos ninguna cosa que de\n<lb/>contar sea que ala estoria de espa\n<lb break=\"no\"/>n<am>̄</am><ex>n</ex>a <choice><abbr><am>ꝑ</am>tenesca</abbr><expan>p<ex>er</ex>tenesca</expan></choice> caso estas cosas que\n<lb/>avemos \n<app>\n<rdg type=\"lit\">dich<seg rend=\"overwritten\"><seg type=\"1\"><gap quantity=\"1\" unit=\"chars\" reason=\"illegible\"/></seg><seg type=\"2\">a</seg></seg>s</rdg>\n<rdg type=\"orig\">dich<gap quantity=\"1\" unit=\"chars\" reason=\"illegible\"/>s</rdg>\n<rdg type=\"mod\" resp=\"\">dichas</rdg>\n</app>\n pasaron. mas ago\n<lb break=\"no\"/>ra dexa la estoria de fablar del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey\n<lb/>don alfon<am>̄</am><ex>so</ex> de castiella <am>⁊</am><ex>e</ex> torna aco<am>̄</am><ex>n</ex>\n<lb break=\"no\"/>tar del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don ferrando de leon⸫ </ab></div>\n<lb/><div n=\"\"><head rend=\"h1\" n=\"Rubricunex\"><space quantity=\"3\" unit=\"lines\"/></head>\n<lb/><ab n=\"\"><hi rend=\"init3unex\">e</hi>nel trezeno an<am>̄</am><ex>n</ex>o del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>egna\n<lb break=\"no\"/>do del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don ferrando de\n<lb/>leon que fue enla era de\n<lb/>mill <am>⁊</am><ex>e</ex> dozientos <am>⁊</am><ex>e</ex> Nueue an<am>̄</am><ex>n</ex>os. q<am>ᵅ</am><ex>ua</ex>n\n<lb break=\"no\"/>do andaua el an<am>̄</am><ex>n</ex>o dela encarnaçion\n<lb/>en mill <am>⁊</am><ex>e</ex> çiento <am>⁊</am><ex>e</ex> setenta <am>⁊</am><ex>e</ex> vno. E\n<lb/>el del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>egnado del <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don alfon<am>̄</am><ex>so</ex>\n<lb/>de castilla en doze. este <choice><abbr><am><g>R</g></am></abbr><expan><ex>rr</ex></expan></choice>ey don\n<lb/>ferrando fijo del enperador fue muy\n<lb/>piadoso <am>⁊</am><ex>e</ex> alegre <am>⁊</am><ex>e</ex> esforçado <am>⁊</am><ex>e</ex> mucho\n\n</ab></div>\n\n\n</root>"
